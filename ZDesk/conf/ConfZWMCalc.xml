<?xml version="1.0" encoding="GB2312"?><Conf>

<!-- 程序使用 单独配置项 -->
<procConf>
<procMode value="common"/> <!-- common 普通程序 abc 农行模式 用于程序同步数据及兼容不同用户特殊要求 -->
<reCalculateTime valueBegin="" valueEnd=""/> <!-- 重新计算指定时间内的数据 的开始结束时间 不重算需设置为空 -->
<logLevel value="DEBUG"/> 
<!-- 控制日志级别 DEBUG INFO WARN ERROR 
级别顺序如下 DEBUG < INFO < WARN  < ERROR
程序使用解释
DEBUG:调试信息
INFO:正常输出信息
WARN:有可能是问题需要分析
ERROR:严重问题需要处理
-->
<threadNumberW value="5"/> 
<threadNumberR value="5"/> 
<threadNumber value="6"/>  <!-- 程序使用的计算线程数量  为-1时程序根据任务动态调控 -->
<isMultiSkill value="true"/>  <!-- 座席是否对多技能组  true或false -->
<minGranularity value="7"/>  <!-- 程序最小时间粒度 8:1刻钟（目前不支持）7:半小时 6:小时  -->
<forwardInterval value="20"/>  <!-- 为减少跨时段数据更新而延迟的数据更新时间 
数据会以粒度对应整数点+延后时间完成数据更新，如粒度30分钟 延后15分钟
则数据会在0:15  0:45 1:15 1:45 ....时间点进行数据更新。
其后发生的跨时段的数据 会导致该时段更新 如电话0:40进入0:45数据更新 0:46电话结束 
此时 0：45数据重新更新

 单位:分钟  -->


<queueInfo queueType="2" queueValue="15"/> <!--定义排队 规则 目前支持：
1.大组排队： 此时 valueTime 为排队组组名
2.严格排队:  此时进入系统后  valueTime 时间(秒)内没有被分配到座席的电话视为排队
3.轮询排队： 电话回到最初进入技能组，视为排队
排队应该成为路由的部分规则在ivr类配置信息中体现 
 -->
<shortAband shortAbandTime="10"/>
<!-- 短时放弃时长 大于0秒值 电话在该时长内放弃视为短时放弃 为负值时不使用短时放弃 -->

 
<!-- 同一座席最长连续登录时间 单位：天（不是小时制，是整点制 ） -->
<maxLoginDay value="3"/>
 
<!-- 不初始化该组座席 即最终数据统计不统计该组及该组座席--> 
<virtualSkill value="$11$"/> 

<!-- 一次问题解决率 时间单位：秒 当前电话前指定时间内无同一号码视为一次解决 -->
<oneceSolvSecond value="7200"/>  

<!-- 控制未接来电明细表中 最小统计的未接来电振铃时长 农行14秒 慧聪1秒 -->
<unAnsPeriod value="1"/>

<!-- 需要ftp上传文件是此项设置为true 并设置具体ftp信息  -->
<isFtp value="false"/>  

<posNum value="1"/>

<!--是否计算话费  -->
<isCalcFee value="false"/>  

<!--部分数据模式 不载入会议 一次解决率等  -->
<isOptimizeMode value="true"/>  

<!--不会建立数据文件目录  -->
<isNeedDir value="false"/>  
<!--不统计监听，临签，座席状态流水等  -->
<isLoadDay value="false"/>  

<!--被动载入座席信息 -->
<isLazyMode value="true"/>  


<!--use second Arr -->
<isSecArr value="true"/>  

<isRouteIpMapContain value="false"/>  

<isCallInDISTINCT value="false"/>  

<shiftsGenMode value="physicsMode"/>  
<phySize value="12"/>  


</procConf>

